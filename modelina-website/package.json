{
  "name": "modelina-website",
  "private": true,
  "scripts": {
    "dev": "npm run build:resources && cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "npm run build:resources && next build --no-lint",
    "build:modelina": "cd .. && npm i && npm run build:prod && npm run docs:api",
    "export": "next export",
    "start": "next start",
    "lint": "next lint -c ./.eslintrc",
    "build:resources": "npm run build:examples && npm run build:docs && npm run copy:docs:img",
    "build:examples": "node scripts/build-examples.js",
    "build:docs": "node scripts/build-docs.js",
    "copy:docs:img": "cp -r ../docs/img public/img/docs"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.4.6",
    "@netlify/functions": "^1.4.0",
    "@next/font": "13.1.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.9",
    "cssnano": "^5.1.14",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.5",
    "js-base64": "^3.7.4",
    "lodash": "^4.17.21",
    "lowlight": "^1.12.1",
    "markdown-toc": "^1.2.0",
    "next": "^13.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "15.5.0",
    "rehype-raw": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1",
    "swiper": "^8.3.2",
    "tailwind-merge": "^1.8.1",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/lowlight": "0.0.3",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-syntax-highlighter": "^15.5.6",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "typescript": "4.9.4"
  }
}
