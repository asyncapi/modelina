{
  "asyncapi": "2.0.0",
  "defaultContentType": "application/json",
  "info": {
    "title": "Signup service example (internal)",
    "version": "0.1.0"
  },
  "channels": {
    "/user/signedup": {
      "subscribe": {
        "message": {
          "payload": {
            "type": "object",
            "properties": {
              "email": {
                "type": "string",
                "format": "email"
              }
            }
          }
        }
      }
    }
  }
}